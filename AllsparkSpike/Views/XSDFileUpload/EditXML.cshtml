@section scripts
{
    <script type="text/javascript">
        $(init);

        function init() {
            $(".element").draggable({
                //containment: '#toolbox',
                stack: '#toolbox div',
                cursor: 'move',
                revert: true
            });

            $('.hotspot').droppable({
                //accept: '#toolbox div',
                hoverClass: 'hovered',
                drop: handleCardDrop
            });
        }
        function handleCardDrop(event, ui) {
            //alert('here');
            event.target.innerHTML = ui.draggable.context.outerHTML;
            //$(event.target).addClass('populatedHotspot');
            $(event.target).removeClass('hotspot');
            $(event.target.childNodes[0]).addClass('moo');

            //ui.draggable.addClass('populatedHotspot');
            //ui.draggable.removeClass('hotspot');
        }
    </script>
}
